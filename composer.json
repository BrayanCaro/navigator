{
    "name": "nedwors/laravel-menu",
    "description": "A Laravel package to ease defining app menus",
    "keywords": [
        "nedwors",
        "laravel-menu"
    ],
    "homepage": "https://github.com/nedwors/laravel-menu",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sam Rowden",
            "email": "nedwors@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Nedwors\\LaravelMenu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nedwors\\LaravelMenu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nedwors\\LaravelMenu\\LaravelMenuServiceProvider"
            ],
            "aliases": {
                "LaravelMenu": "Nedwors\\LaravelMenu\\LaravelMenuFacade"
            }
        }
    }
}
